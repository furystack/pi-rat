{
  "name": "common",
  "version": "1.0.3",
  "license": "GPL-2.0-only",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas/*": {
      "import": "./schemas/*"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "create-schemas": "node ./dist/bin/create-schemas.js"
  },
  "devDependencies": {
    "@types/ffprobe": "^1.1.4",
    "@types/node": "^18.16.2",
    "ts-json-schema-generator": "^1.2.0"
  },
  "dependencies": {
    "@furystack/core": "^12.0.2",
    "@furystack/rest": "^5.0.2"
  }
}
